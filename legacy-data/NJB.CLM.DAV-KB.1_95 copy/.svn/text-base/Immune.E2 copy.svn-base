im						 ( memory.cell ( memory.cell1 ) false )		\ PopSim						 ( blast.cell ( blast.cell1 ) false )		\ PopSim						 ( antibody ( antibody1 ) false )			\ PopSim 						  )if.relates              (						\ part.of (  membrane.1 T-cell1 ) true )						\ ( part.of (  membrane.2 B-cell1 ) true )						\ ( part.of (  membrane.3  Macrophae1) true )						\ ( presents ( b-cell1 antigen1 ) false )						( presents ( macrophage1 antigen1 ) false )			\ PopSim						\ ( abuts ( b-cell1 antigen1 ) true )						\ ( abuts ( macrophage1 antigen1 ) true )			 			( abuts ( b-cell1 helper.t-cell1 ) true )						\ ( abuts ( B-cell1 act.h.t-cell1 ) true )						( excretes ( virus1 antigen1 ) false )				\ PopSim						\ ( excited ( virus1 ) false )		turns out not needed						( binds ( macrophage1 antigen1 ) false )			\ PopSim						( binds ( b-cell1 antigen1 ) false )				\ PopSim						( binds ( helper.T-cell1 antigen1 ) false )			\ PopSim						( contains ( b-cell1 antigen1 ) false  )			\ PopSim						( contains ( macrophage1 antigen1 ) false  )		\ PopSim						( presents ( b-cell1 antigen1 ) false )				\ PopSim						( digests ( b-cell1 antigen1 ) false )				\ PopSim						( digests ( macrophage1 antigen1 ) false )			\ PopSim						( secretes ( macrophage1 IL1 ) false )				\ PopSim						( secretes ( macrophage1 csf1 ) false )				\ PopSim						( proliferates ( helper.T-cell1 ) false )			\ PopSim						( antigen.binds ( act.h.t-cell1 b-cell1 ) false )	\ PopSim						( binds ( antibody1 virus1 ) false )				\ PopSim						( ingests ( macrophage1 virus1 ) false )			\ PopSim						( attaches.to ( IL1 cytokine.receptor1 ) false )	\ PopSim						( synthesizes ( activated.b-cell1 cytokine.receptor1 ) false )	\ PopSim						\ ( presents ( macrophage1 antigen1 ) false )	\ jp2     			             )	ca.populations	   (   						\ ( Population.of ( t-cell1  5 ) true )						( Population.of ( b-cell1  33 ) true )						( Population.of ( macrophage1 132  ) true )						( Population.of ( help