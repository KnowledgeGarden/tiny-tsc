{	IL-effects.Rn  Interleukin Effects Rules	editing, cleanup and modification, 7/10-7/17/95 by rbt }c:  interleukin-4.effects.a		level  			basic		sub.of			phys.process		instance.of 	process.rule 		my.creator 		jwp		context 		immunology		if.actors		( ( IL-4 ( *IL-4 ) true )						  ( IL-1 ( *IL-1 ) true )						  ( IL-6 ( *IL-6 ) true )						  ( IL-8 ( *IL-8 ) true )						  ( TNF-alpha ( *TNF-alpha ) true )						  ( macrophage.tumoricidal.activity ( *macrophage.tumoricidal.activity ) true )						  ( macrophage.colony.formation ( *macrophage.colony.formation ) true ) )		then.create  	( *class.II.mhc )		then.actors		( ( class.II.mhc ( *class.II.mhc ) true )						\ ( mouse ( *mouse ) false )						\ ( IL-4 ( *IL-4 ) false )						  )		then.relates	( ( inhibits.production.of ( *IL-4 *IL-1 ) true )						  ( inhibits.production.of ( *IL-4 *IL-6 ) true )						  ( inhibits.production.of ( *IL-4 *IL-8 ) true )						  ( inhibits.production.of ( *IL-4 *TNF-alpha ) true )						  ( inhibits.production.of ( *IL-4 *macrophage.colony.formation ) true )						  ( induces (  *IL-4 *macrophage.tumoricidal.activity ) true )						  ( induces ( *IL-4 *MHC2.production ) true ) )		then.say		"IL-4 inhibits production of IL-1 IL-6 IL-8 TNF-alpha and macrophages and also induces claas.II.mhc"c:	interleukin-10.effects.a		level  			basic		sub.of			phys.process		instance.of		process.rule 		my.creator 		jwp		context 		immunology	    if.actors		( ( IL-10 ( *IL-10 ) true )	    				  ( IL-4 ( *IL-4 ) true )						  ( IL-1 ( *IL-1 ) true )						  ( IL-6 ( *IL-6 ) true )						  ( IL-8 ( *IL-8 ) true )						  ( IL-12 ( *IL-12 ) true )						  ( GM-csf ( *GM-csf ) true )						  ( TNF-alpha ( *TNF-alpha ) true )						  ( IFN-gamma ( *IFN-gamma ) true )						  ( class.II.mhc ( *class.II.mhc ) true ) )		then.actors 	( ( IL-10 ( *IL-10 ) false )						\ ( zero.hours ( *zero.hours ) false )						\ ( three.hours ( *three.hours ) false )						\ ( six.hours ( *six.hours ) false )						\ ( nine.hours ( *nine.hours ) false )						\ ( 24.hours ( *24.hours ) false ) 						\ ( mouse ( *mouse ) false )						( activated.splenocyte ( *activated.splenocyte ) false )  )	    then.relates 	( \ ( inhibited.by ( *IL-10 *IL-10 ) true )	    				\ ( inhibited by ( *IFN-gamma *IL-10 ) true )					 	( inhibits.production.of (  *IL-10 *IL-1 ) true )						( inhibits.production.of ( *IL-10 *IL-6 ) true )						( inhibits.production.of ( *IL-10 *IL-8 ) true ) 						( inhibits.production.of ( *IL-10 *IL-10 ) true )						( inhibits.production.of ( *IL-10 *GM-csf ) true )						( inhibits.production.of ( *IL-10 *TNF-alpha ) true )						( inhibits.production.of ( *IL-10 *IFN-gamma ) true )						( inhibits.production.of ( *IL-10 *class.II.mhc ) true )						( inhibits.production.of ( *IL-10 *IL-12 ) true ) )		then.say		"IL-10 inhibits production of IL-1 IL-6 IL-8 Il-10 IL-12 GM-csf TNF-alpha IFN-gamma and class.II.mhc"		 						c:	interleukin-10.effects.b		level  			basic		sub.of			phys.process		instance.of 	process.rule 		my.creator 		jwp		context 		immunology	    if.actors		( ( IL-10 ( *IL-10 ) true )	    				  ( B-cell-ab ( *B-cell-ab ) true ) ) 							then.create 	( *IgA *IgG *IgM )		then.actors		( ( IgG ( *IgG ) true )						  ( IgA ( *IgA ) true )						  ( IgM ( *IgM ) true )						  \ ( zero.hours ( *zero.hours ) false )						  \ ( three.hours ( *three.hours ) false )						  \ ( six.hours ( *six.hours ) false )						  \ ( nine.hours ( *nine.hours ) false )						  \ ( 24.hours ( *24.hours ) false )	    				  \ ( B-cell-ab ( *B-cell-ab ) false ) 	    				  \ ( mouse ( *mouse ) false )	    				  ( IL-10 ( *IL-10 ) false ) )	    then.relates	( ( induces.B-cell.secretion.of  ( *IL-10 *IgG ) true )						  ( induces.B-cell.secretion.of  ( *IL-10 *IgA ) true )						  ( induces.B-cell.secretion.of  ( *IL-10 *IgM ) true ) ) 	    then.say		" IL-10 induces B-cell to secrete IgG IgA IgM. "						c:   interleukin-12.effects.a		level  		basic		sub.of		phys.process		instance.of process.rule 		my.creator jwp		context 	immunology	    if.actors		( ( IL-12 ( *IL-12 ) true ) 	    				  ( macrophage ( *macrophage ) true )	    				  ( NK-cell ( *NK-cell ) true )	    				  ( activated.T-cell ( *activated.T-cell ) true ) )		then.create		( *IFN-gamma *IL-10 )		then.actors 	( ( IFN-gamma ( *IFN-gamma ) true )						  ( IL-10 ( *IL-10 ) true )						  \ ( zero.hours ( *zero.hours ) false )						  \ ( three.hours ( *three.hours ) false )						  \ ( six.hours ( *six.hours ) false )						  \ ( nine.hours ( *nine.hours ) false )						  \ ( 24.hours ( *24.hours ) false )						  \ ( mouse ( *mouse ) false ) 						  ( IL-12 ( *IL-12 ) false ) )		then.relates  	( ( increases.proliferation.of  ( *IL-12 *NK-cell ) true )						( increases.proliferation.of  ( *IL-12 *activated.T-cell ) true )						( activates ( *IL-12 *macrophage ) true )						( induces.secretion.of ( *IL-12 *IFN-gamma ) true )						( induces.secretion.of ( *IL-12 *IL-10 ) true ) )		then.say		"IL-12 increases proliferation of NK-cell and activated.T-cell, induces secretion of IL-10 and IL-12 and activates macrophage "c:  interferon-gamma.effects.a		level  			basic		sub.of			phys.process		instance.of		process.rule 		my.creator 		jwp		context 		immunology	    if.actors		( ( IFN-gamma ( *IFN-gamma ) true ) )		then.create		( *class.I.mhc *class.II.mhc )		then.actors 	( ( class.I.mhc ( *class.I.mhc ) true )					   	  ( class.II.mhc ( *class.II.mhc ) true )					   	  \ ( zero.hours ( *zero.hours ) false )						  \ ( three.hours ( *three.hours ) false )						  \ ( six.hours ( *six.hours ) false )						  \ ( nine.hours ( *nine.hours ) false )						  \ ( 24.hours ( *24.hours ) false )  						  ( mouse ( *mouse ) false )						  ( IFN-gamma ( *IFN-gamma ) false ) )		then.relates   	( ( activates ( *IFN-gamma *MHC1.expression ) true )					   	  ( activates ( *IFN-gamma *MHC2.expression ) true )						  ( activates ( *IFN-gamma *macrophage ) true )							  ( induces ( *IFN-gamma *IL-12 ) true )						  ( induces ( *IFN-gamma *NK-cell ) true ) 						  ( inhibited.by  ( *IL-10 *IFN-gamma ) true ) )		then.say		" IFN-gamma activates mhc1 and mhc2 expression and induces NK-cell "{	These rules amount to mere declarative statements and have virtually no inferential	usefulness due to lack of causal process behaviorsc:	poly.I:C.treatment.effects		level  			basic		sub.of			phys.process		instance.of 	process.rule 		my.creator 		jwp		context			immunology	    if.actors		( ( Poly.I:C.treatment ( *Poly.I:C.treatment ) true ) 	    				( virus ( *emcv-mm ) true ) )	    then.actors		( ( Poly.I:C.treatment ( *Poly.I:C.treatment ) false ) 	    				( virus ( *emcv-mm ) false ) )	    then.states		( ( somewhat.protected ( *male.mouse ) true )	    				( somewhat.protected ( *female.mouse ) true ) )	    then.say		"poly.I:C.treatment provides some protection to emcv.mm effected male and female mice"	    c:	anti-IL-10.treatment.effects		level  		basic		sub.of		phys.process		instance.of process.rule 		my.creator jwp		context 	immunology	    if.actors		( ( anti-IL-10.treatment ( *anti-IL-10.treatment ) true ) 	    				( female.mouse ( *female.mouse ) true ) 	    				( virus ( *emcv-mm ) true ) )	    then.states		( ( survives.longer ( *female.mouse ) true ) )	    then.actors		( ( anti-IL-10.treatment ( *anti-IL-10.treatment ) false ) 	    				( female.mouse ( *female.mouse ) false ) 	    				( virus ( *emcv-mm ) false ) )	    then.say		" anti-IL-10 treatment leads to prolonged survival in emcv-mm infected female mice"c:	poly.I:C.treatment.and.anti-IL-10.treatment.effects.a		level  		basic		sub.of		phys.process		instance.of process.rule 		my.creator jwp		context 	immunology	    if.actors		( ( anti-IL-10.treatment ( *anti-IL-10.treatment ) true )	    				( virus ( *emcv-d ) true )	    				( male.mouse ( *male.mouse ) true )	    				( Poly.I:C.treatment ( *Poly.I:C.treatment ) true ) )	    then.states		( ( resistant.to.diabetes ( *emcv-d.infected.male.mouse ) true )					    				)	    then.actors		( ( Poly.I:C.treatment ( *Poly.I:C.treatment ) false )	    				( virus ( *emcv-d ) false )	    				( male.mouse ( *male.mouse ) false ) 	    				( anti-IL-10.treatment ( *anti-IL-10.treatment ) false ) )	    then.say		"combined poly I:C and Anti-IL-10 treatments result in a resistance to diabetes in emcv-d infected male mice"    				 c:	poly.I:C.treatment.and.anti-IL-10.treatment.effects.b		level  		basic		sub.of		phys.process		instance.of process.rule 		my.creator jwp		context 	immunology	    if.actors		( ( anti-IL-10.treatment ( *anti-IL-10.treatment ) true )	    				( virus ( *emcv-mm ) true )	    				( female.mouse ( *female.mouse ) true )	    				( Poly.I:C.treatment ( *Poly.I:C.treatment ) true ) )	    then.states		( 					    				( survives.longer ( *female.mouse ) true )	    				( reduced.mortality ( *female.mouse ) true ) )	    then.actors		( ( Poly.I:C.treatment ( *Poly.I:C.treatment ) false )	    				( virus ( *emcv-mm ) false )	    				( female.mouse ( *female.mouse ) false ) 	    				( anti-IL-10.treatment ( *anti-IL-10.treatment ) false ) )	    then.say		"combined poly I:C and Anti-IL-10 treatments results in prolonged survival and reduction of mortality in emcv-mm infected female mice"}	    				 