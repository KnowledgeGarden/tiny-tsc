l1 helper.t-cell1 ) true )			\ ( abuts ( B-cell1 act.h.t-cell1 ) true )			( excretes ( virus1 antigen1 ) false )				\ PopSim			\ ( excited (  virus1 ) false )		turns out not needed			( binds ( macrophage1 antigen1 ) false )			\ PopSim			( binds ( b-cell1 antigen1 ) false )				\ PopSim			( binds ( helper.T-cell1 antigen1 ) false )			\ PopSim			( contains ( b-cell1 antigen1 ) false  )			\ PopSim			( contains ( macrophage1 antigen1 ) false  )		\ PopSim			( presents ( b-cell1 antigen1 ) false )				\ PopSim			( digests ( b-cell1 antigen1 ) false )				\ PopSim			( digests ( macrophage1 antigen1 ) false )			\ PopSim			( secretes ( macrophage1 IL1 ) false )				\ PopSim			( secretes ( macrophage1 csf1 ) false )				\ PopSim			( proliferates ( helper.T-cell1 ) false )			\ PopSim			( antigen.binds ( act.h.t-cell1 b-cell1 ) false )	\ PopSim			( binds ( antibody1 virus1 ) false )				\ PopSim			( ingests ( macrophage1 virus1 ) false )			\ PopSim			( attaches.to ( il1 cytokine.receptor1 ) false )	\ PopSim			\ ( presents ( macrophage1 antigen1 ) false )	\ jp2			 )	ca.populations	( 				\ ( Population.of ( t-cell1 5 ) true )				( Population.of ( b-cell1 33 ) true )				( Population.of ( macrophage1 32 ) true )				( Population.of ( helper.T-cell1 31 ) true )				\ ( Population.of ( act.h.t-cell1 20 ) true )				\ ( Population.of ( a.b-cell1 19 ) true )				\ ( Population.of ( b.ab.t-cell1 18 ) true )				\ ( Population.of ( plasma1 17 ) true )				\ ( Population.of ( memory1 16 ) true )				\ ( Population.of ( blast.cell1 25 ) true )				\ ( Population.of ( stem-cell1 5 ) true )				( Population.of ( virus1 12 ) true )				\ ( Population.of ( cytokine.receptor1 30 ) true )				\ ( Density.of ( antigen1 6 ) true )				\ ( Density.of ( antibody1 7 ) true )				\ ( Concentration.of ( csf1 0 ) true )				\ ( Concentration.of ( cytokine1 80 ) true )				\ ( Concentration.of ( IL1 0 ) true )				\ ( Concentration.of ( IL2 0 ) true )			  )c:	Immune.E1.Exp	instance.of		experiment	context		human.immunology	initial.conditions	Immune.E1	ca.dimensions	24 16	\ dimensions of the grid	ca.neighborhood	5 5		\ dimensions of a neighborhood	ca.turbulance	10	ca.iterations	50 2	\ total iterations; iterations per result	ca.results		nil		\ place to store results\ ____________________________________________________________ TASKS\ Use TASK.I1 to run only the QP envisionment.c:	TASK.I1	instance.of		agendatask	topic			discovery	do.task		fillin	on.concept		immune.e1	on.slot		next.episode	params		nil	priority		900	my.sources		jp2	reasons		focus.of.attentionCURRENT>AGENDA{\ Use TASK.PS.I1 to run translate the KB for PopSim and send it off to be executed.  When PopSim\ has run the requested number of iterations it will pause and the QP envisionment will be started.\ Remember PopSim must be running before TSC is engaged on this task.c:	TASK.PS.I1	instance.of		agendatask	topic			data.exploration	do.task		study	on.concept		Immune.E1.Exp	on.slot		ca.results	params		( no.data )	priority		750	my.sources		dhf	reasons		focus.of.attention}