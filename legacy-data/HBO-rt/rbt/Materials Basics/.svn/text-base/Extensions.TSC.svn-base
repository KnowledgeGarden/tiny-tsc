	my.vars		*truth	algorithm	( do	( bindq *truth F )						( display *elem print )						( if.true	( isa?  *elem 'atom  )							( cond	( ( notnull? ( value of *elem 'electronegativity ) )									  ( do ( display "notnull" print )									  		( bindq *truth T ) ) )									( ( notnull? ( value of ( value.of *elem 'synonym ) 															'electronegativity ) )									  ( do ( display "notnull2" print )									  	( bindq *truth T ) ) )									( T null ) ) )						( return *truth ) )						{ ==================================		&&&&	BIG NOTE:	Both of these can fail if electronegativity has "nodata" in the slot==================================== }c:	ELECTROPOS?	instance.of	flow.pred	i.take		sx	i.give		flag	arguments	*elem	my.vars		*truth *x	algorithm	( do	( bindq *truth F )						( if.true	( isa?  *elem 'atom )							\ we deduce it is electropos because it does							\ not have an electronegativity value							( cond 	( ( notnull? ( value of *elem 'electronegativity ) )									  ( bindq *truth F ) )									( ( notnull? ( value of ( value.of *elem 'synonym )															 'electronegativity ) )									  ( bindq *truth F ) )									( T ( bindq *truth T ) ) ) )						( return *truth ) )						COMMENT;						c:	ELECTRONEGATIVITY	sub.of	information.slot	legal.values	number symbol	{ =============================	NOTE	By using definitions here, we can hand any symbol to	electro negative or positive.element and see if it is one of them	Ionic.Bond requires an electroneg and electropos element	These definitions thus support reasoning such as:		can these two elements form an ionic bond?	See BOND.TYPE in Materials.T================================ }c:	ELEMENT	sub.of	thing	c:	ELECTRONEGATIVE.ELEMENT		sub.of			element		definition		electroneg?		disjoint.from	electropositive.element			c:	ELECTROPOSITIVE.ELEMENT		sub.of			element		definition		electropos?		disjoint.from	electronegative.element		+c:	ATOM	sub.of	element