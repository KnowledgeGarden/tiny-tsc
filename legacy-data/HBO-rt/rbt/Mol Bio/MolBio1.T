						+c:	TRP-R.OPERATOR				sub.of						gene.operator				my.creator					jp2 pdk				promoters.controlled			trp-r.promoter			  		+c:	SPACER			sub.of					dna.segment			my.creator				jp2 pdk			can.exist.in.isolation		F						+c:	TRP.LEADER				sub.of				spacer				my.creator			jp2 pdk				\   "See Trp.Operon.ED102"				\				+c:	TRP.LEADER.ED102					instance.of				trp.leader	\ &&&& instance ????					my.creator			jp2 pdk								  				\   "See  trp.delta.LC1419"				\				+c:	TRP.LEADER.LC1419					instance.of				trp.leader	\ &&&& instance ????				 	my.creator			jp2 pdk								\   "See  trp.delta.LD1412"				\				+c:	TRP.LEADER.LD1412					instance.of		trp.leader	\ &&&& instance ????					my.creator	jp2 pdk		+c:	PROMOTER			sub.of						dna.segment			my.creator					jp2 pdk			can.exist.in.isolation			F 			component.object.classes		promoter.polymerase.binding.site 			generic.component.objects	*polymerase.binding.site 				+c:	DEF.OFF.PROMOTER				sub.of					promoter				my.creator				jp2 pdk				receptive.to.polymerase	no			 			+c:	DEF.ON.PROMOTER				sub.of					promoter				my.creator				jp2 pdk				receptive.to.polymerase	yes							+c:	TRP.PROMOTER					sub.of					def.on.promoter					my.creator				jp2 pdk					starting.base.offset		-37					ending.base.offset		3				 				+c:	TRP-R.PROMOTER					sub.of						def.on.promoter					my.creator					jp2 pdk			 				+c:	REGULON					sub.of				dna.segment					my.creator			jp2 pdk								+c:	RESTRICTION.SITE					sub.of					dna.segment					my.creator				jp2 pdk					can.exist.in.isolation		F				\ ---------------------------------------------------------------------------------------------------------------GENES			+c:	GENE			sub.of						dna.segment			my.creator					jp2 pdk			can.exist.in.isolation		no			polypeptide.coded.for		nil 			rna.segment.produced		rna.gene					\ appear to control developmental fate of groups of cells			\			+c:	HOMEOTIC.GENE				sub.of			gene				my.creator		jp2					\ mutated proto-oncogene now involved with cancer			\			+c:	ONCOGENE				sub.of			gene				my.creator		jp2							\ performs some vital normal cell function, but can mutate and 			\ be involved with cancer			\			+c:	PROTO-ONCOGENE				sub.of			gene				my.creator		jp2					\ there must be other types of GENEs, of which TRP-A, etc are subs or views of						+c:	TRP-A				sub.of					gene				my.creator				jp2 pdk				starting.base.offset		5581				ending.base.offset		6684				polypeptide.coded.for		trp-synthetase.alpha						+c:	TRP-B				sub.of					gene				my.creator				jp2 pdk				starting.base.offset		4688				ending.base.offset		5881				polypeptide.coded.for		trp-synthetase.beta						+c:	TRP-C				sub.of					gene				my.creator				jp2 pdk				starting.base.offset		3324				ending.base.offset		4688				polypeptide.coded.for		phosphoribosyl-anthranilate-isomerase							\   "See  trp.delta.LC1419"				\				+c:	TRP-C.LC1419					instance.of		trp-c					my.creator		jp2 pdk			 			+c:	TRP-D				sub.of					gene				my.creator				jp2 pdk				starting.base.offset		1225 				ending.base.offset		3324 				polypeptide.coded.for	anthranilate-synthetase.component.ii 							\   "See  trp.delta.ED24"				\				+c:	TRP-D.ED24					instance.of		trp-d					my.creator	jp2 pdk							\   "See  trp.delta.LD1412"				\				+c:	TRP-D.LD1412					instance.of		trp-d					my.creator		jp2 pdk								\   "See Trp.Operon.ED102"				\				+c:	TRP-D.ED102					instance.of		trp-d					my.creator		jp2 pdk						+c:	TRP-E				sub.of					gene				my.creator				jp2 pdk				starting.base.offset		163				ending.base.offset		1225				polypeptide.coded.for		anthranilate-synthetase.component.i							\   "See  trp.delta.ED24"				\				+c:	TRP-E.ED24					instance.of				trp-c					my.creator				jp2 pdk						+c:	TRP-R.GENE				sub.of					gene				my.creator				jp2 pdk				polypeptide.coded.for		trp-r.polypeptide							+c:	TRP.TRNA.SYNTHETASE.GENE				sub.of			gene				my.creator		jp2 pdk			+c:	ENTITIES.WITH.DISSOLVED.COMPONENTS	sub.of					chemical	my.creator				jp2 pdk\ ********	STRUCTURAL.RELATIONS	(FORALL *COMPONENT (AND (MEMB *COMPONENT\									(XGET.VALUES OBJECT (QUOTE COMPONENT.OBJECTS))\								(XADD.PAIRED.VALUE *COMPONENT (QUOTERELATION.TO.CONTAINERS) \									OBJECT (QUOTE DISSOLVED))\ forall instances, add slot/value " relation.to.container dissolved"	+c:	EXPERIMENT		sub.of			entities.with.dissolved.components		my.creator		jp2 pdk		+c:	MEDIA		sub.of			entities.with.dissolved.components		my.creator		jp2 pdk	\ RBOUND=reversibly bound\+c:	ENTITIES.WITH.RBOUND.COMPONENTS	sub.of				chemical	\ Karp used "everything" here	my.creator			jp2 pdk\	*inherit.along>COMMENT:	structural.relations	(forall *component 								(and (memb *component									(xget.values object (quote component.objects)))) 									((xadd.paired.value *component 										(quote relation.to.containers) object (quote rbound))))))\ inherit to all instances ( *component ???? ) the slot/value "relation.to.containers (object rbound)"COMMENT;	+c:	RNA.SEGMENT	sub.of					nucleic.acid.segment	my.creator				jp2 pdk	decomposition.disjoint	transfer.rna messenger.rna  ribosomal.rna 	\   " We'll map a number of different types of DNA segments to this mRNA segment."	\	+c:	MISC.MRNA.SEGMENT		sub.of						rna.segment		my.creator					jp2 pdk		can.exist.in.isolation			F 	+c:	RIBOSOMAL.RNA		sub.of					rna.segment		my.creator				jp2 pdk			+c:	5S.RRNA			sub.of						ribosomal.rna			my.creator					jp2 pdk			sedimentation.coefficient	5				+c:	16S.RRNA			sub.of						ribosomal.rna			my.creator					jp2 pdk			sedimentation.coefficient		16				+c:	23S.RRNA			sub.of						ribosomal.rna			my.creator					jp2 pdk			sedimentation.coefficient		23				+c:	RNA.GENE		sub.of						rna.segment		my.creator					jp2 pdk		can.exist.in.isolation			F	+c:	TRANSFER.RNA		sub.of					rna.segment		my.creator				jp2 pdk		redundancy				0		synonym				trna			+c:	UNCHARGED.TRNA			sub.of						transfer.rna			my.creator					jp2 pdk					+c:	TRP.TRNA				sub.of						uncharged.trna				my.creator					jp2 pdk				redundancy					1		+c:	CHARGED.TRNA			sub.of			transfer.rna			my.creator		jp2 pdk		 			+c:	CHARGED.TRP.TRNA				sub.of						charged.trna				my.creator					jp2 pdk				component.object.classes		trp.trna trp 				generic.component.objects	*trp.trna *trp	 +c:	OPERON	sub.of			regulon	my.creator		jp2 pdk	state.variables	concentration	+c:	TRP.LEADER.REGION		sub.of						operon		my.creator					jp2 pdk		component.object.classes		trp.promoter trp.operator trp.leader 		component.object.classes		trp.terminator		generic.component.objects	*pro *op *leader *term 		*inherit>					( FIRST.SEGMENT *PRO ) 		*inherit>>					( *PRO 3PRIME.SEGMENT *OP )		*inherit>>					( *OP 3PRIME.SEGMENT *LEADER )		*inherit>>					( *LEADER 3PRIME.SEGMENT *TERM )		*inherit>>					( *OP PROMOTERS.CONTROLLED *PRO )		+c:	TRP.OPERON		sub.of						operon		my.creator					jp2 pdk		component.object.classes		trp.promoter trp.operator trp.leader 		component.object.classes		dribosome.binding.site trp-e dstop.codon 		component.object.classes		dribosome.binding.site trp-d dstop.codon 		component.object.classes		dribosome.binding.site trp-c dstop.codon 		component.object.classes		dribosome.binding.site trp-b dstop.codon		component.object.classes		dribosome.binding.site trp-a dstop.codon 		component.object.classes		trp.terminator		generic.component.objects	*pro *op *leader *drbs1 *trpe *dsc1 *drbs2 		generic.component.objects	*trpd *dsc2 *drbs3 *trpc *dsc3 *drbs4 *trpb 		generic.component.objects	*dsc4 *drbs5 *trpa *dsc5 *term		*inherit>					( first.segment *pro )		*inherit>>					( *op promoters.controlled *pro )		*inherit>>					( *pro 3prime.segment *op )		*inherit>>					( *op 3prime.segment *leader )		*inherit>>					( *leader 3prime.segment *drbs1 )		*inherit>>					( *drbs1 3prime.segment *trpe )		*inherit>>					( *trpe 3prime.segment *dsc1 )		*inherit>>					( *dsc1 3prime.segment *drbs2 )		*inherit>>					( *drbs2 3prime.segment *trpd )		*inherit>>					( *trpd 3prime.segment *dsc2 )		*inherit>>					( *dsc2 3prime.segment *drbs3 )		*inherit>>					( *drbs3 3prime.segment *trpc )		*inherit>>					( *trpc 3prime.segment *dsc3 )		*inherit>>					( *dsc3 3prime.segment *drbs4 )		*inherit>>					( *drbs4 3prime.segment *trpb )		*inherit>>					( *trpb 3prime.segment *dsc4 )		*inherit>>					( *dsc4 3prime.segment *drbs5 )		*inherit>>					( *drbs5 3prime.segment *trpa )		*inherit>>					( *trpa 3prime.segment *dsc5 )		*inherit>>					( *dsc5  3prime.segment *term )	 		+c:	TRP.OPERON.ED102.S		sub.of						operon		my.creator					jp2 pdk		component.object.classes		trp.promoter trp.operator trp.leader.ed102		component.object.classes		trp-d.ed102 trp-c trp-b trp-a trp.terminator		generic.component.objects	*pro *op *leader *trpd *trpc *trpb *trpa  *term		*inherit>					( first.segment *pro  )		*inherit>>					( *op promoters.controlled *pro )		*inherit>>					( *pro 3prime.segment *op )		*inherit>>					( *op 3prime.segment *leader )		*inherit>>					( *leader 3prime.segment *trpd )		*inherit>>					( *trpd 3prime.segment *trpc )		*inherit>>					( *trpc 3prime.segment *trpb )		*inherit>>					( *trpb 3prime.segment *trpa )		*inherit>>					( *trpa 3prime.segment *term )			+c:	TRP.OPERON.ED102		sub.of						operon		my.creator					jp2 pdk		component.object.classes		trp.promoter trp.operator trp.leader.ed102 		component.object.classes		trp-d.ed102 dribosome.binding.site 		component.object.classes		trp-c dstop.codon dribosome.binding.site 		component.object.classes		trp-b dstop.codon dribosome.binding.site 		component.object.classes		trp-a dstop.codon trp.terminator		generic.component.objects	*pro *op *leader *trpd *drbs3 *trpc *dsc3 		generic.component.objects	*drbs4 *trpb *dsc4 *drbs5 *trpa *dsc5 *term		*inherit>					( first.segment *pro )		*inherit>>					( *op promoters.controlled *pro )		*inherit>>					( *pro 3prime.segment *op )		*inherit>>					( *op 3prime.segment *leader )		*inherit>>					( *leader 3prime.segment *trpd )		*inherit>>					( *trpd 3prime.segment *drbs3 )		*inherit>>					( *drbs3 3prime.segment *trpc )		*inherit>>					( *trpc 3prime.segment *dsc3 )		*inherit>>					( *dsc3 3prime.segment *drbs4 )		*inherit>>					( *drbs4 3prime.segment *trpb )		*inherit>>					( *trpb 3prime.segment *dsc4 )		*inherit>>					( *dsc4 3prime.segment *drbs5 )		*inherit>>					( *drbs5 3prime.segment *trpa )		*inherit>>					( *trpa 3prime.segment *dsc5 )		*inherit>>					( *dsc5 3prime.segment *term )		 		+c:	TRP.OPERON.ED24		sub.of						operon		my.creator					jp2 pdk		component.object.classes		trp.promoter trp.operator trp.leader 		component.object.classes		trp-e.ed24 trp-d.ed24 trp-c trp-b trp-a 		component.object.classes		trp.terminator		generic.component.objects	*pro *op *leader *trpe *trpd *trpc *trpb  *trpa *term		*inherit>					( first.segment *pro )		*inherit>>					( *op promoters.controlled *pro )		*inherit>>					( *pro 3prime.segment *op )		*inherit>>					( *op 3prime.segment *leader )		*inherit>>					( *leader  3prime.segment *trpe )		*inherit>>					( *trpe  3prime.segment *trpd )		*inherit>>					( *trpd 3prime.segment *trpc )		*inherit>>					( *trpc 3prime.segment *trpb )		*inherit>>					( *trpb  3prime.segment *trpa )		*inherit>>					( *trpa  3prime.segment *term )	 	+c:	TRP-R.OPERON		sub.of						operon		my.creator					jp2 pdk		component.object.classes		trp-r.promoter trp-r.operator 		component.object.classes		trp-r.gene trp-r.terminator		generic.component.objects	*pro *op *gene *term		*inherit>					( first.segment *pro )		*inherit>>					( *pro 3prime.segment *op )		*inherit>>					( *op 3prime.segment *gene )		*inherit>>					( *gene  3prime.segment *term )	  	+c:	TRP.OPERON.LD1412		sub.of						operon			my.creator					jp2 pdk		component.object.classes		trp.promoter trp.operator trp.leader 		component.object.classes		trp-d.ld1412 trp-c trp-b trp-a trp.terminator		generic.component.objects	*pro *op *leader *trpd *trpc *trpb *trpa *term		*inherit>					( first.segment *pro )		*inherit>>					( *op promoters.controlled *pro )		*inherit>>					( *pro 3prime.segment *op )		*inherit>>					( *op 3prime.segment *leader )		*inherit>>					( *leader 3prime.segment *trpd )		*inherit>>					( *trpd 3prime.segment *trpc )		*inherit>>					( *trpc  3prime.segment *trpb )		*inherit>>					( *trpb  3prime.segment *trpa )		*inherit>>					( *trpa 3prime.segment *term )	 	+c:	TRP.OPERON.LC1419		sub.of						operon		my.creator					jp2 pdk		component.object.classes		trp.promoter trp.operator trp.leader trp-a 		component.object.classes		trp-b trp-c.lc1419 trp.terminator		generic.component.objects	*pro *op *leader *trpa *trpb *trpc *term		*inherit>					( first.segment *pro )		*inherit>>					( *op  promoters.controlled *pro )		*inherit>>					( *pro  3prime.segment *op )		*inherit>>					( *op  3prime.segment *leader )		*inherit>>					( *leader  3prime.segment *trpa )		*inherit>>					( *trpa 3prime.segment *trpb )		*inherit>>					( *trpb  3prime.segment *trpc )		*inherit>>					( *trpc  3prime.segment *term )COMMENT:	when an object is instantiated, structural.relations get executed, setting up some slots	It appears that, in an episode, the actors would include *PRO, *OP, etc.	The structural.relations code serves as an inheritance mechanism.  Instead, when some	new object is built, the building code should use the *inherit code in the frame.	To do that, some way to bind up the vbls will be needed--the code building the new object must	bind up the vbls in the inheritance slots.	(generic.component.objects ($pro $op $leader $term))	(structural.relations ((put.value object (quote first.segment) $pro) 						(put.value $pro (quote 3prime.segment) $op)						(put.value $op (quote 3prime.segment) $leader) 						(put.value $leader (quote 3prime.segment) $term) 						(put.value $op (quotepromoters.controlled) $pro)))COMMENT;\ ---------------------------------------------------------------------------------------------------------------GEOGRAPHY+c:	DOMAIN	sub.of					idea	\ &&&&	part.of					protein	my.creator				jp2	my.ref					bio.4+c:	ACTIVE.SITE	sub.of						idea	\ &&&&	part.of						protein	my.creator					jp2 pdk	can.exist.in.isolation			F	+c:	RNA.ACTIVE.SITE		sub.of						rna.segment active.site		my.creator					jp2 pdk				+c:	START.CODON			sub.of						rna.active.site			my.creator					jp2 pdk			potential.interacting.objects	ribosome				+c:	STOP.CODON			sub.of						rna.active.site			my.creator					jp2 pdk			potential.interacting.objects	ribosome				+c:	RIBOSOME.BINDING.SITE			sub.of						rna.active.site			my.creator					jp2 pdk			potential.interacting.objects	ribosome	  	+c:	DNA.ACTIVE.SITE		sub.of					dna.segment active.site		my.creator				jp2 pdk			+c:	DNA.BINDING.SITE			sub.of					dna.active.site			my.creator				jp2 pdk				+c:	PROMOTER.POLYMERASE.BINDING.SITE				sub.of						dna.binding.site				my.creator					jp2 pdk				potential.interacting.objects	rna-polymerase							+c:	TRP.OPERATOR.REPRESSOR.BINDING.SITE				sub.of						dna.binding.site				my.creator					jp2 pdk				potential.interacting.objects	trp-repressor		+c:	DRIBOSOME.BINDING.SITE			sub.of					dna.active.site			my.creator				jp2 pdk			rna.segment.produced	ribosome.binding.site			+c:	DSTART.CODON			sub.of				dna.active.site		 	my.creator			jp2 pdk	 		+c:	DSTOP.CODON			sub.of					dna.active.site			my.creator				jp2 pdk			rna.segment.produced	stop.codon	+c:	PROTEIN.ACTIVE.SITE		sub.of						active.site		my.creator					jp2 pdk		part.of						protein			\ NIL PROTOHISTORY suggests a "discovery" activity to fillin the slot ********		\		+c:	PROTEIN.BINDING.SITE			sub.of						protein.active.site			my.creator					jp2 pdk				+c:	RNA-POLYMERASE.PROMOTER.BINDING.SITE				su